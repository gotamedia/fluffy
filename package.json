{
  "name": "@gotamedia/fluffy",
  "description": "Fluffy UI",
  "version": "0.15.1",
  "private": false,
  "main": "./index.js",
  "module": "./index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "finalize-package": "./scripts/finalize-package.sh",
    "local": "npm run build && yalc publish dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "./scripts/storybook.sh && build-storybook",
    "babel": "babel temp_dist --source-root temp_dist --out-dir dist --extensions .js --copy-files --quiet",
    "types": "ttsc --project ./temp_root/tsconfig.json",
    "release": "standard-version && npm run finalize-package",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "start:ci": "serve --no-clipboard --listen ${PORT:-8080}",
    "cy:open": "cypress open --component --browser chrome",
    "cy:run": "cypress run --component --browser chrome"
  },
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@contexts(.*)$": "<rootDir>/src/contexts$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1"
    },
    "collectCoverageFrom": [
      "src/*/**",
      "!src/*/**/index.js"
    ]
  },
  "browser": {
    "@components": false
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@react-theming/storybook-addon": "^1.1.6",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.14",
    "@types/react-datepicker": "^4.4.2",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/parser": "^5.30.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "chai-colors": "^1.0.1",
    "cypress": "^10.3.1",
    "cypress-real-events": "^1.7.1",
    "eslint": "^8.18.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.13",
    "font-color-contrast": "^11.1.0",
    "lodash": "^4.17.21",
    "npm": "^8.15.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "storybook": "^6.5.9",
    "styled-components": "^5.3.5",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.73.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "pigeon-maps": "^0.21.0",
    "polished": "^4.2.2",
    "react-datepicker": "^4.8.0",
    "react-intersection-observer": "^9.4.0",
    "scroll-into-view-if-needed": "^2.2.29"
  }
}
